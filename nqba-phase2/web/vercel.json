{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "env": {
    "NEXT_PUBLIC_API_BASE": "https://api.goliathomniedge.com"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.goliathomniedge.com/v2/:path*"
    }
  ]
}
