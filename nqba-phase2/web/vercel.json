{
  "version": 2,
  "name": "nqba-phase2-web",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE": "https://api.goliathomniedge.com"
  },
  "functions": {
    "app/**/*.tsx": {
      "includeFiles": "**"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.goliathomniedge.com/v2/:path*"
    }
  ]
}
